    <title>Adfectus Engine</title>
    <authors>Vlad 'Cryru' Abadzhiev</authors>
    <owners>Vlad 'Cryru' Abadzhiev</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
