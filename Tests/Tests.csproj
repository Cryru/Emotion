        <EmbeddedResource Include="Assets\Embedded\EmbedText.txt" />
        <EmbeddedResource Include="Assets\Shaders\DepthTest.frag" />
        <EmbeddedResource Include="Assets\Shaders\DepthTest.xml" />
    </ItemGroup>

